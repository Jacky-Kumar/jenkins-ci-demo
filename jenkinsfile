pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Code checked out from GitHub'
            }
        }

        stage('Build') {
            steps {
                bat 'echo Building project'
                bat 'dir'
            }
        }

        stage('Test') {
            steps {
                bat 'type hello.txt'
            }
        }
    }
}
